{
  "name": "angular2-workflow",
  "version": "2.0.0-beta.2",
  "author": "Zsolt R. MOLNAR",
  "description": "Docker-based Angular 2 Webpack Starter",
  "main": "src/app/main.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/antonybudianto/angular-webpack-starter.git"
  },
  "engines": {
    "node": ">=4.3",
    "npm": "~3.0.0"
  },
  "scripts": {
    "start": "scripts/start.sh",
    "start:dll": "run-s build:dll start",
    "pretest": "npm run clean:reports",
    "test": "karma start",
    "tslint": "tslint",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "prebuild": "npm run clean:build",
    "build:prod": "scripts/build-prod.sh",
    "build:dll": "webpack --config config/webpack/webpack.dll.js --profile --bail",
    "serve:build": "run-s build serve",
    "clean": "run-p clean:*",
    "clean:build": "rimraf dist/*",
    "clean:reports": "rimraf reports/*",
    "clean:dll": "rimraf dll-dist",
    "coveralls": "coveralls < ./reports/coverage/lcov.info"
  },
  "keywords": [
    "angular2"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/antonybudianto/angular-webpack-starter/issues"
  },
  "dependencies": {
    "@angular/compiler-cli": "2.4.5",

    "@types/core-js": "0.9.35",
    "@types/node": "6.0.51",
    "@types/webfontloader": "^1.6.27",
    "@types/redux-logger": "^2.6.34",

    "ng2-redux": "^5.1.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "angular2-router-loader": "^0.3.4",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "3.0.0-beta.18",
    "compression-webpack-plugin": "0.3.2",
    "copy-webpack-plugin": "^4.0.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "istanbul-instrumenter-loader": "0.2.0",
    "null-loader": "^0.1.1",
    "coffee-loader": "^0.7.2",
    "css-loader": "^0.26.0",
    "file-loader": "^0.9.0",
    "url-loader": "^0.5.7",
    "json-loader": "^0.5.4",
    "pug-ng-html-loader": "^1.0.10",
    "pug-html-loader": "^1.1.0",
    "pug-plugin-ng": "^0.0.2",
    "progress-bar-webpack-plugin": "1.9.0",
    "raw-loader": "^0.5.1",
    "postcss-loader": "^1.1.1",
    "sass-loader": "^4.1.0",
    "style-loader": "^0.13.1",
    "webfontloader": "^1.6.27",
    "tslint-loader": "^3.2.1",
    "webpack-dev-server": "^2.1.0-beta.11",
    "add-asset-html-webpack-plugin": "^1.0.2",
    "webpack": "^2.2.0",
    "webpack-merge": "^1.0.2",
    "@ngtools/webpack": "^1.2.4",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "deep-freeze": "0.0.1",
    "babel-preset-angular": "^6.0.15",

    "breakpoint-sass": "^2.7.1",
    "compass-mixins": "^0.12",
    "normalize.css": "*",
    "node-sass": "3.13.1",
    "postcss-cssnext": "^2.9.0",
    "precss": "^1.4.0",

    "font-awesome": "^4.7.0",
    "foundation-sites": "6.2.4",
    "reflect-metadata": "^0.1.8",
    "zone.js": "0.6.26",
    "jquery": "^3.1.1",

    "ajv-keywords": "1.1.1",
    "coffee-script": "1.x",
