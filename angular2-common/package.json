    "dotenv": "^2.0.0",
    "npm-run-all": "^3.1.2",
    "pug": "^2.0.0-beta6",
    "rimraf": "^2.5.4",
    "tslint": "~4.0.2",
    "typescript": "2.1.5",
    "compression": "^1.6.2",
    "ts-node": "^1.7.0",

    "karma": "~1.3.0",
    "karma-coverage": "~1.1.1",
    "karma-jasmine": "~1.0.2",
    "karma-phantomjs-launcher": "~1.0.2",
    "karma-remap-coverage": "^0.1.2",
    "karma-webpack": "1.8.0",
    "karma-mocha-reporter": "^2.0"
  }
}