{
    "name": "workflows-angular",
    "version": "1.0.0",
    "description": "Angular and Webpack 2 Starter",
    "scripts": {
        "postinstall": "npm run build:dll",
        "rimraf": "rimraf",
        "webdev": "webpack-dev-server",
        "webdev:hmr": "npm run webdev -- --hotOnly --hot --inline",
        "webpack": "webpack",
        "webdriver-manager": "webdriver-manager",
        "webdriver:update": "npm run webdriver-manager update",
        "webdriver:start": "npm run webdriver-manager start",
        "protractor": "protractor",
        "pretest": "npm run lint",
        "pretest:once": "npm run lint",
        "pretest:once:ci": "npm run lint",
        "unittest:once": "karma start",
        "unittest:once:ci": "karma start",
        "unittest": "karma start",
        "ci": "npm run unittest:once:ci",
        "ci:testall": "&& npm run unittest:once:ci",
        "tslint": "tslint --project tsconfig.json",
        "clean": "npm run clean:compile && npm run rimraf -- node_modules artifacts/*",
        "clean:dist": "npm run rimraf -- artifacts/dist/* artifacts/.awcache",
        "clean:dll": "npm run rimraf -- artifacts/dll",
        "clean:compile": "npm run rimraf -- \"artifacts/compiled\" ",
        "start": "npm run server:dev",
        "start:hmr": "npm run server:hmr",
        "server:dev": "npm-run-all -p -r webdev",
        "server:hmr": "npm-run-all -p -r webdev:hmr",
        "server:universal": "nodemon /app/artifacts/dist/server.js",
        "debug:build": "node-nightly --inspect --debug-brk node_modules/webpack/bin/webpack.js",
        "build": "npm run build:prod",
        "build:aot": "npm run build:aot:prod",
        "build:aot:prod": "npm run clean:dist && webpack",
        "build:aot:dev": "npm run clean:dist && webpack",
        "build:dev": "npm run clean:dist && webpack",
        "build:dll": "npm run clean:dll && webpack",
        "build:prod": "npm run clean:dist && webpack",
        "build:server": "npm run build:server:prod",
        "build:server:aot": "npm run build:server:aot:prod",
        "build:server:aot:dev": "webpack",
        "build:server:aot:prod": "webpack",
        "build:server:dev": "npm run clean:dist && webpack",
        "build:server:prod": "npm run clean:dist && webpack",
        "build:universal": "npm run build:universal:prod",
        "build:universal:aot": "npm-run-all -p build:aot build:server:aot",
        "build:universal:aot:dev": "npm-run-all -p build:aot:dev build:server:aot:dev",
        "build:universal:dev": "npm run clean:dist && webpack",
        "build:universal:prod": "npm run clean:dist && webpack",
        "watch": "npm run watch:dev",
        "watch:aot": "npm-run-all -p -r \"build:aot:dev -- --watch\"",
        "watch:dev": "npm-run-all -p -r \"build:dev -- --watch\"",
        "watch:prod": "npm-run-all -p -r \"build:prod -- --watch\"",
        "watch:universal": "npm-run-all -p -r \"build:universal -- -- --watch\"",
        "universal": "npm run build:universal && npm run server:universal",
        "universal:aot": "npm run build:universal:aot && npm run server:universal",
        "universal:watch": "npm run build:universal && npm-run-all -p -r watch:universal server:universal",
        "doc:build": "compodoc -d ./artifacts/docs -p tsconfig.json",
        "doc:serve": "compodoc -s -r 8092 -d ./artifacts/docs -p tsconfig.json",
        "doc:buildandserve": "compodoc -s -r 8092 -d ./artifacts/docs -p tsconfig.json"
    },
    "dependencies": {
        "@angular/cdk": "^5.2.4",
        "@angular/platform-server": "~5.2.9",
        "@angular/tsc-wrapped": "~4.4.6",
        "@angularclass/hmr": "2.1.3",
        "@angularclass/hmr-loader": "3.0.4",
        "@nguniversal/express-engine": "5.0.0-beta.5",
        "body-parser": "^1.18.2",
        "compression": "^1.7.1",
        "cookie-parser": "1.4.3",
        "core-js": "2.5.3",
        "express": "~4.16.2",
        "hammerjs": "~2.0.8",
        "ip": "~1.1.5",
        "methods": "1.1.2",
        "@compodoc/compodoc": "^1.0.0"
    },
    "devDependencies": {
        "script-ext-html-webpack-plugin": "^2.0.1",
        "@ngtools/webpack": "1.10.2",
        "@types/body-parser": "^1.17.0",
        "@types/compression": "~0.0.35",
        "@types/cookie-parser": "~1.4.1",
        "@types/express": "~4.11.0",
        "@types/express-serve-static-core": "~4.11.0",
        "@types/hammerjs": "~2.0.35",
        "@types/ip": "~0.0.30",
        "@types/mime": "~2.0.0",
        "@types/selenium-webdriver": "~3.0.8",
        "@types/serve-static": "~1.13.1",
        "@types/webpack": "^4.4.0",
        "angular-router-loader": "~0.8.1",
        "angular2-template-loader": "~0.6.2",
        "awesome-typescript-loader": "^5.0.0",
        "compression-webpack-plugin": "~1.1.3",
        "copy-webpack-plugin": "^4.0.0",
        "css-loader": "~0.28.7",
        "es6-promise": "~4.2.2",
        "es6-promise-loader": "~1.0.2",
        "html-webpack-plugin": "^3.2.0",
        "imports-loader": "^0.8.0",
        "istanbul-instrumenter-loader": "~3.0.0",
        "json-loader": "~0.5.7",
        "karma": "~2.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage": "~1.1.1",
        "karma-jasmine": "~1.1.1",
        "karma-mocha-reporter": "~2.2.5",
        "karma-remap-coverage": "^0.1.0",
        "karma-sourcemap-loader": "~0.3.7",
        "karma-webpack": "^3.0.0",
        "node-sass": "^4.9.0",
        "nodemon": "^1.0.0",
        "npm-run-all": "~4.1.2",
        "raw-loader": "~0.5.1",
        "rimraf": "~2.6.2",
        "sass-loader": "^7.0.0",
        "source-map-loader": "~0.2.3",
        "string-replace-loader": "^2.0.0",
        "to-string-loader": "~1.1.5",
        "ts-helpers": "~1.1.2",
        "ts-node": "^6.0.0",
        "tslint-loader": "^3.0.0",
        "watch-run": "~1.2.5",
        "webpack": "^4.8.0",
        "webpack-cli": "^2.1.3",
        "webpack-bundle-analyzer": "^2.9.1",
        "webpack-dev-middleware": "^3.0.0",
        "webpack-dev-server": "^3.1.0",
        "webpack-md5-hash": "^0.0.6",
        "webpack-merge": "~4.1.1",
        "webpack-node-externals": "^1.6.0",
        "pug": "^2.0.0-beta6",
        "pug-html-loader": "^1.0.0",
        "mini-css-extract-plugin": "^0.4.0",
        "html-loader": "^0.5.1",
        "apply-loader": "^2.0.0",
        "url-loader": "^1.0.1",

        "breakpoint-sass": "^2.7.1",
        "compass-mixins": "^0.12",
        "normalize.css": "*",
        "postcss-preset-env": "^5.1.0",
        "precss": "^3.1.2",
        "autoprefixer": "^8.5.0",
        "cssnano": "^3.10.0",
        "postcss-import": "^11.0.0",
        "resolve-url-loader": "^2.1.0",
        "postcss-apply": "^0.10.0",
        "postcss-responsive-type": "^1.0.0",
        "postcss-loader": "^2.0.6",

        "uglifyjs-webpack-plugin": "^1.2.0",
        "imagemin-webpack-plugin": "^2.0.0"
    }
}