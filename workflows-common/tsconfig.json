{
    "compilerOptions": {
        "target": "es5",
        "declaration": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true,
        "importHelpers": true,
        "strictNullChecks": false,
        "removeComments": true,
        "sourceMap": true,
        "lib": [
            "es6",
            "dom"
        ]
    }
}